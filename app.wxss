/**app.wxss**/
page {
    font-family: 'Source Han Sans CN';
    font-style: normal;
    font-size: 32rpx;
    
}
.background{
    position: fixed;
    width: 100vw;
    height: 100vh;
    z-index: -999;
    background: linear-gradient(-140deg, #00ffff, #ffc0cb);
}
.backArrow{
    width: 20rpx;
    height: 20rpx;
    border-top: solid 6rpx #000;
    border-left: solid 6rpx #000;
    transform: rotate(-45deg);
}
.roundImage{
    border: solid 1rpx #000;
    border-radius: 50%;
}
